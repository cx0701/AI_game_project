    <tags>Unity Package Manager Tool</tags>
    <packageTypes>
      <packageType name="DotnetTool" />
    </packageTypes>
    <repository type="git" url="https://github.com/GlitchEnzo/NuGetForUnity" commit="f8f1902ebec77284fd4ac83d043e0ef664d31831" />
  </metadata>
</package>